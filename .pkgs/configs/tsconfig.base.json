{
  "extends": [
    "@tsconfig/strictest/tsconfig.json"
  ],
  "compilerOptions": {
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "moduleDetection": "force",
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "checkJs": false,
    "erasableSyntaxOnly": true,
    "types": [
      "tsl/patches"
    ],
    "plugins": [
      {
        "name": "tsl/plugin"
      }
    ]
  }
}
