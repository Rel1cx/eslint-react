{
  "extends": [
    "@tsconfig/strictest/tsconfig.json",
    "@tsconfig/vite-react/tsconfig.json"
  ],
  "compilerOptions": {
    "target": "ES2021",
    "lib": [
      "ES2021",
      "DOM",
      "DOM.Iterable"
    ],
    "types": [
      "vite/client"
    ],
    "erasableSyntaxOnly": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
