{
  "name": "eslint-plugin-react-naming-convention",
  "version": "1.5.2-beta.2",
  "description": "ESLint React's ESLint plugin for naming convention related rules.",
  "homepage": "https://github.com/rel1cx/eslint-react",
  "bugs": {
    "url": "https://github.com/rel1cx/eslint-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rel1cx/eslint-react",
    "directory": "packages/plugins/eslint-plugin-react-naming-convention"
  },
  "license": "MIT",
  "author": "Eva1ent<let@ik.me>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "./dist",
    "./package.json"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin swc3 && cp dist/index.d.ts dist/index.d.mts",
    "build:docs": "typedoc",
    "lint:publish": "publint",
    "lint:type": "tsc --noEmit",
    "publish": "pnpm run build && pnpm run lint:publish"
  },
  "dependencies": {
    "@eslint-react/ast": "workspace:*",
    "@eslint-react/core": "workspace:*",
    "@eslint-react/jsx": "workspace:*",
    "@eslint-react/shared": "workspace:*",
    "@eslint-react/tools": "workspace:*",
    "@eslint-react/types": "workspace:*",
    "@typescript-eslint/parser": ">=6.20.0",
    "@typescript-eslint/scope-manager": ">=6.20.0",
    "@typescript-eslint/type-utils": ">=6.20.0",
    "@typescript-eslint/types": ">=6.20.0",
    "@typescript-eslint/utils": ">=6.20.0",
    "string-ts": "2.0.0"
  },
  "devDependencies": {
    "effect": "2.2.3"
  },
  "peerDependencies": {
    "eslint": ">=8.56.0",
    "typescript": ">=5.3.3"
  },
  "engines": {
    "bun": ">=1.0.15",
    "node": ">=18.18.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
