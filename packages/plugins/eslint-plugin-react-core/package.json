{
  "name": "eslint-plugin-react-core",
  "version": "1.5.11-next.11",
  "description": "ESLint React's ESLint plugin for React related rules.",
  "homepage": "https://github.com/rel1cx/eslint-react",
  "bugs": {
    "url": "https://github.com/rel1cx/eslint-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rel1cx/eslint-react",
    "directory": "packages/plugins/eslint-plugin-react-core"
  },
  "license": "MIT",
  "author": "Eva1ent<let@ik.me>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "./dist",
    "./package.json"
  ],
  "scripts": {
    "build": "tsup",
    "build:docs": "typedoc",
    "lint:publish": "publint",
    "lint:type": "tsc --noEmit",
    "publish": "pnpm run build && pnpm run lint:publish"
  },
  "dependencies": {
    "@eslint-react/ast": "workspace:*",
    "@eslint-react/core": "workspace:*",
    "@eslint-react/jsx": "workspace:*",
    "@eslint-react/shared": "workspace:*",
    "@eslint-react/tools": "workspace:*",
    "@eslint-react/types": "workspace:*",
    "@eslint-react/var": "workspace:*",
    "@typescript-eslint/parser": "^7.8.0",
    "@typescript-eslint/scope-manager": "^7.8.0",
    "@typescript-eslint/type-utils": "^7.8.0",
    "@typescript-eslint/types": "^7.8.0",
    "@typescript-eslint/utils": "^7.8.0",
    "string-ts": "2.1.1",
    "ts-api-utils": "1.3.0",
    "valibot": "0.30.0"
  },
  "devDependencies": {
    "effect": "3.1.2"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^9.1.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "bun": ">=1.0.15",
    "node": ">=18.18.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
