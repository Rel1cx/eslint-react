{
  "name": "@eslint-react/core",
  "version": "0.9.6",
  "description": "ESLint React's ESLint utility module for static analysis of React core API and Patterns.",
  "homepage": "https://github.com/rel1cx/eslint-react",
  "bugs": {
    "url": "https://github.com/rel1cx/eslint-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rel1cx/eslint-react",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": "Eva1ent<let@ik.me>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "./package.json"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin swc3 && cp dist/index.d.ts dist/index.d.mts",
    "build:docs": "typedoc && dprint fmt ./docs/**/*.md",
    "lint:publish": "publint",
    "lint:type": "tsc --noEmit",
    "publish": "pnpm run build && pnpm run lint:publish"
  },
  "dependencies": {
    "@eslint-react/ast": "workspace:*",
    "@eslint-react/jsx": "workspace:*",
    "@eslint-react/shared": "workspace:*",
    "@eslint-react/tools": "workspace:*",
    "@eslint-react/types": "workspace:*",
    "@typescript-eslint/scope-manager": "6.14.0",
    "@typescript-eslint/type-utils": "6.14.0",
    "@typescript-eslint/types": "6.14.0",
    "@typescript-eslint/utils": "6.14.0"
  }
}
